<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <Configurations>Debug;Release;Translate</Configurations>
    <Platforms>AnyCPU;x64</Platforms>
	<UseWindowsForms>true</UseWindowsForms>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="icon\delete.png" />
    <None Remove="icon\exchange.png" />
    <None Remove="Logo\icons8-google-translate-new-100.ico" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="icon\delete.png" />
    <Resource Include="icon\exchange.png" />
    <Resource Include="Logo\icons8-google-translate-new-100.ico" />
  </ItemGroup>

</Project>
